{
	"name": "element-plus-pro",
	"private": true,
	"license": "ISC",
	"workspaces": [
		"packages/*",
		"play",
		"docs"
	],
	"scripts": {
		"dev": "pnpm -C play dev",
		"docs:dev": "pnpm run -C docs dev",
		"gen:version": "tsx scripts/gen-version.ts",
		"update:version": "tsx scripts/update-version.ts",
		"clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
		"clean:dist": "rimraf dist",
		"build": "pnpm --filter \"@element-plus/**\" build",
		"typecheck:web": "vue-tsc -p tsconfig.web.json --composite false --noEmit"
	},
	"devDependencies": {
		"@element-plus/icons-vue": "^2.3.1",
		"@element-plus/pro-button": "workspace:^",
		"@element-plus/pro-components": "workspace:^",
		"@element-plus/pro-utils": "workspace:^",
		"@esbuild-kit/cjs-loader": "^2.4.4",
		"@pnpm/find-workspace-packages": "^6.0.9",
		"@types/gulp": "^4.0.17",
		"@types/node": "^20.8.9",
		"@vitejs/plugin-vue": "^4.4.0",
		"@vitejs/plugin-vue-jsx": "^3.0.2",
		"element-plus": "^2.4.1",
		"esbuild": "^0.20.2",
		"eslint": "^8.52.0",
		"fs-extra": "^11.1.1",
		"glob": "^10.3.12",
		"gulp": "^4.0.2",
		"mime": "^4.0.1",
		"picocolors": "^1.0.0",
		"rimraf": "^5.0.5",
		"rollup": "^4.14.3",
		"rollup-plugin-visualizer": "^5.11.0",
		"sass": "^1.69.5",
		"screenfull": "^6.0.2",
		"tsup": "^8.0.2",
		"tsx": "^3.14.0",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"vue": "^3.3.7",
		"vue-draggable-plus": "^0.3.5",
		"vue-tsc": "^1.8.27",
		"vue3-styled-components": "^1.2.1"
	},
	"dependencies": {
		"@vue/shared": "^3.3.8"
	}
}
