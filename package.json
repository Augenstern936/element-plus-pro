{
  "name": "element-plus-pro",
  "version": "0.0.6",
  "private": true,
  "license": "ISC",
  "packageManager": "pnpm@8.10.2",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "cz": "git add . && czg",
    "dev": "pnpm -C play dev",
    "docs:dev": "pnpm run -C docs dev",
    "build": "npx nx run-many -t build --projects=@element-plus-ui/**",
    "graph": "npx nx graph",
    "changelog": "npx nx release changelog",
    "dry-run": "npx nx release --dry-run --first-release --verbose",
    "gen:tag": "npx nx release version --git-tag",
    "push:tags": "git push origin --tags",
    "first:publish": "npx nx release publish --first-release && pnpm push:tags",
    "release": "npx nx release && pnpm push:tags",
    "publish": "npx nx release publish && pnpm push:tags",
    "gen:version": "tsx scripts/gen-version.ts",
    "update:version": "tsx scripts/update-version.ts",
    "format": "prettier --write --cache .",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@element-plus-ui/pro-hooks": "workspace:^",
    "@element-plus-ui/pro-types": "workspace:^",
    "@element-plus-ui/pro-utils": "workspace:^",
    "@element-plus/icons-vue": "^2.3.1",
    "@esbuild-kit/cjs-loader": "^2.4.4",
    "@nx/js": "^19.4.1",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@pnpm/types": "^10.0.0",
    "@popperjs/core": "^2.11.8",
    "@swc-node/register": "^1.10.3",
    "@swc/core": "^1.6.13",
    "@types/gulp": "^4.0.17",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.8.9",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/shared": "^3.2.37",
    "@vueuse/core": "^10.7.0",
    "consola": "^3.2.3",
    "cz-git": "^1.9.4",
    "czg": "^1.9.4",
    "element-plus": "^2.7.6",
    "eslint": "^9.9.0",
    "fetch-mock": "^10.0.8",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.12",
    "gulp": "^4.0.2",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "msw": "^2.3.1",
    "nx": "19.3.0",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-visualizer": "^5.11.0",
    "sass": "^1.69.5",
    "tsup": "^8.0.2",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-babel-import": "^2.0.5",
    "vite-plugin-chunk-split": "^0.5.0",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vitest": "^1.6.0",
    "vue": "^3.3.7",
    "vue-demi": "^0.14.8",
    "vue-draggable-plus": "^0.3.5",
    "vue3-styled-components": "^1.2.1"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): v${version}",
      "push": true,
      "commit": true,
      "tag": true,
      "requireCommits": false,
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true,
      "draft": true
    },
    "npm": {
      "publish": false,
      "ignoreVersion": false
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
